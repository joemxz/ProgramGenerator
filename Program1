<![CDATA[[GRT(Cfg_FiltTC,0.0) CPT(Wrk_Alpha,1.0/(1.0+Wrk_ScanTime/Cfg_FiltTC)) ,LEQ(Cfg_FiltTC,0.0) CLR(Wrk_Alpha) ,XIO(Wrk_SelPVInfNaN) CPT(Wrk_FiltPV,(Wrk_Alpha*Wrk_FiltPV)+((1.0-Wrk_Alpha)*Wrk_UnfiltPV)) ];]]>
